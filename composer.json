{
    "name": "jornatf/msgraph-mailer",
    "description": "A Laravel Package to send emails with Microsoft Graph API.",
    "keywords": [
        "jornatf",
        "laravel",
        "msgraph-mailer"
    ],
    "homepage": "https://github.com/jornatf/msgraph-mailer",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan",
            "email": "me@jordnat.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jornatf\\MsGraphMailer\\": "src/",
            "Jornatf\\MsGraphMailer\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jornatf\\MsGraphMailer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jornatf\\MsGraphMailer\\MsGraphMailerServiceProvider"
            ],
            "aliases": {
                "MsGraphMailer": "Jornatf\\MsGraphMailer\\Facades\\MsGraphMailer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
