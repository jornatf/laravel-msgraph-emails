{
	"name": "jornatf/msgraph-mailer",
	"description": "A Laravel Package to send emails with Microsoft Graph API.",
	"keywords": [
		"laravel",
		"api",
		"microsoft graph api",
		"microsoft",
		"email",
		"send mail",
		"mailer"
	],
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Jornatf\\MsGraphMailer\\": "src/"
		}
	},
	"require": {
		"php": ">= 7.4",
		"guzzlehttp/guzzle": "^7.5.0"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Jornatf\\MsGraphMailer\\Providers\\MsGraphMailerProvider"
			]
		}
	}
}
